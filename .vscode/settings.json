{
	"editor.formatOnSave": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"stylelint.enable": true,
	"stylelint.packageManager": "npm",
	"stylelint.validate": ["css"],
	"[css]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "stylelint.vscode-stylelint",
		"editor.codeActionsOnSave": {
			"source.fixAll.stylelint": "explicit"
		}
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": false
	},
	"files.autoSave": "off",
	"files.exclude": {
		"plugins/**/*": true
	},
	"[php]": {
		"editor.formatOnSave": false,
		"editor.trimAutoWhitespace": false,
		"files.trimTrailingWhitespace": false,
		"files.insertFinalNewline": false
	},
	"files.trimTrailingWhitespace": false,
	"files.trimTrailingWhitespaceInRegexAndStrings": true,
	"phpcs.executablePath": "./vendor/squizlabs/php_codesniffer/bin/phpcs",
	"phpcbf.executablePath": "./vendor/squizlabs/php_codesniffer/bin/phpcbf",
	"phpcbf.standard": "./phpcs.xml.dist",
	"phpcs.standard": "./phpcs.xml.dist",
	"[xml]": {
		"editor.defaultFormatter": "DotJoshJohnson.xml"
	},
	"workbench.colorCustomizations": {
		"titleBar.activeBackground": "#f9e64f",
		"activityBar.activeBackground": "#fbed80",
		"activityBar.background": "#fbed80",
		"activityBar.foreground": "#15202b",
		"activityBar.inactiveForeground": "#15202b99",
		"activityBarBadge.background": "#06b9a5",
		"activityBarBadge.foreground": "#15202b",
		"commandCenter.border": "#15202b99",
		"sash.hoverBorder": "#fbed80",
		"statusBar.background": "#f9e64f",
		"statusBar.foreground": "#15202b",
		"statusBarItem.hoverBackground": "#f7df1e",
		"statusBarItem.remoteBackground": "#f9e64f",
		"statusBarItem.remoteForeground": "#15202b",
		"titleBar.activeForeground": "#15202b",
		"titleBar.inactiveBackground": "#f9e64f99",
		"titleBar.inactiveForeground": "#15202b99"
	},
	"peacock.color": "#f9e64f"
}
